<h1>Borrado de usuarios</h1>

<form action="/usuarios/eliminate" method="POST">
<fieldset>
	<% if (locals.mensaje) { %>
		<p><strong><%= locals.mensaje %></strong></p>
	<% } %>
	<legend>Nombre del usuario a borrar</legend>
	<div><label>Nombre:</label> <input type="text" name="username" /></div>
	<button type="submit">Eliminar</button>
</fieldset>
